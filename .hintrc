{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "button-type": "off",
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  }
}